{
  "name": "urban-manufact-cons",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/swiper": "^4.4.2",
    "@types/xlsx": "0.0.36",
    "ali-oss": "^6.0.1",
    "antd": "^3.9.2",
    "bizcharts": "^3.1.10",
    "build-js": "^1.0.0",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.14",
    "js-base64": "^2.4.5",
    "leaflet": "^1.3.1",
    "less": "^2.5.3",
    "less-loader": "^4.1.0",
    "loadable-components": "^2.1.0",
    "lodash": "^4.17.10",
    "md5-typescript": "^1.0.5",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "mobx-react-devtools": "^5.0.1",
    "moment": "^2.22.2",
    "mqtt": "^2.17.0",
    "node-sass-chokidar": "^1.3.0",
    "qrcode": "^1.2.0",
    "qrcode.react": "^0.8.0",
    "react": "^16.3.2",
    "react-barcode": "^1.3.2",
    "react-ckeditor-component": "^1.0.7",
    "react-data-export": "^0.4.2",
    "react-dom": "^16.3.2",
    "react-leaflet": "^1.9.1",
    "react-motion": "^0.5.2",
    "react-motion-drawer": "^3.1.0",
    "react-responsive": "^4.1.0",
    "react-route-guard": "^1.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "3.1.0",
    "ssr-window": "^1.0.1",
    "swiper": "^4.5.0",
    "urlencode": "^1.1.0",
    "uuid": "^3.3.2",
    "xlsx-oc": "^1.0.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired --max_old_space_size=6144 start --scripts-version react-scripts-ts",
    "build": "npm-run-all build-css build-js",
    "build-js": "react-app-rewired --max_old_space_size=6144 build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "lint": "tslint -p .",
    "precommit-msg": "echo-cli 'Running lint ...'",
    "test-cov": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom --coverage",
    "eject": "react-scripts-ts eject",
    "page": "node ./scripts/new_page",
    "flavor": "node scripts/flavor.js",
    "deploy": "node scripts/deploy.js",
    "deploy:test": "npm run flavor test && npm-run-all build-css build-js && npm run flavor rollback && npm run deploy test",
    "deploy:prod": "npm run flavor production && npm-run-all build-css build-js && npm run flavor rollback && npm run deploy production"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/js-base64": "^2.3.1",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.1.0",
    "@types/qrcode": "^0.8.1",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-leaflet": "^1.1.5",
    "@types/react-motion": "0.0.26",
    "@types/react-responsive": "^3.0.1",
    "@types/react-router-dom": "^4.2.6",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-mobx-async-action": "^0.0.6",
    "echo-cli": "^1.0.8",
    "node-ssh": "^5.1.2",
    "npm-run-all": "^4.1.3",
    "pre-commit": "^1.2.2",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.5.2",
    "replace": "^1.0.0",
    "ts-import-plugin": "^1.4.4",
    "typescript": "3.1.6"
  }
}
